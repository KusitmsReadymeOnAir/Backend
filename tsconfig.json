{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "outDir": "src",
        "esModuleInterop": true,
        "strict": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "sourceMap": true,
        "noImplicitThis":false
    },
    "exclude": [
        "node_modules"
    ],
    "include":[
        "src/**/*.ts"
    ]
}